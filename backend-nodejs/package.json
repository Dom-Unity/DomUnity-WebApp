{
    "name": "domunity-backend-nodejs",
    "version": "1.0.0",
    "description": "DomUnity gRPC Backend - Node.js Implementation",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "generate-proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:. --grpc_out=grpc_js:. -I ../proto ../proto/domunity.proto",
        "test": "jest --coverage",
        "test:unit": "jest test/unit.test.js",
        "test:integration": "jest test/integration.test.js"
    },
    "keywords": [
        "grpc",
        "postgresql",
        "domunity"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@grpc/grpc-js": "^1.9.14",
        "@grpc/proto-loader": "^0.7.10",
        "bcrypt": "^5.1.1",
        "dotenv": "^16.3.1",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "grpc-tools": "^1.12.4",
        "nodemon": "^3.0.2",
        "@jest/globals": "^29.7.0",
        "jest": "^29.7.0"
    }
}