{
    "name": "domunity-frontend",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@grpc/grpc-js": "^1.9.14",
        "@grpc/proto-loader": "^0.7.10",
        "google-protobuf": "^4.0.0",
        "grpc-web": "^1.5.0",
        "pbjs": "^0.0.14",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.1",
        "react-scripts": "5.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "generate-proto": "protoc --proto_path ../proto --plugin=protoc-gen-js=\"../frontend/tools/protobuf-javascript-4.0.0-win64/bin/protoc-gen-js.exe\" --plugin=protoc-gen-grpc-web=\"../frontend/tools/protoc-gen-grpc-web.exe\" --js_out=import_style=commonjs:./src/proto --grpc-web_out=import_style=commonjs,mode=grpcwebtext:./src/proto ../proto/domunity.proto",
        "generate-proto-mac": "protoc --proto_path ../proto --plugin=protoc-gen-js=\"../frontend/tools/protobuf-javascript-4.0.0-osx-x86_64/bin/protoc-gen-js\" --plugin=protoc-gen-grpc-web=\"../frontend/tools/protoc-gen-grpc-web-2.0.2-darwin-x86_64\" --js_out=import_style=commonjs:./src/proto --grpc-web_out=import_style=commonjs,mode=grpcwebtext:./src/proto ../proto/domunity.proto"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}